DEFINE_VARIABLE PYRET = 0; - ;
DEFINE_VARIABLE PYCOND = 1; - ;
DEFINE_VARIABLE ROADL = 0; - ;
DEFINE_VARIABLE STEER_CTRL = -0.279167; - ;
DEFINE_VARIABLE XPREV = 17.407; - ;
DEFINE_VARIABLE YPREV = -9.84859; - ;
DEFINE_VARIABLE LERROR = -1.6; m ; Lateral Error from path
DEFINE_VARIABLE TOTERR = 1.6; - ;
DEFINE_VARIABLE TCOUNT = 1; - ;
DEFINE_VARIABLE L_FORWARD = 24; m ; Distance to view point
DEFINE_VARIABLE ITERS = 1; - ;
DEFINE_VARIABLE PRINTVALS = 0; - ;
DEFINE_VARIABLE BESTVAL = 0; - ;
DEFINE_VARIABLE BESTVALERROR = 0; - ;

DEFINE_OUTPUT Xpreview = 17.407; m ; X coordinate of preview point 
SET_OUTPUT_COMPONENT XPREVIEW Steer control preview point;
DEFINE_OUTPUT Ypreview = -9.84859; m ; Y coordinate of preview point 
SET_OUTPUT_COMPONENT YPREVIEW Steer control preview point;

!------------------------------------------------------------------------------------
! EQUATIONS IN (AT THE START OF EVERY TIME STEP)
!------------------------------------------------------------------------------------
EQ_IN PYCOND = IF_GT_0_THEN(FMOD(TCOUNT, 50), 0, 1);
EQ_IN TCOUNT = TCOUNT + 1;
EQ_IN PYRET = PYTHON(PYCOND, "steercontrol.steer_param", "OPEN", None, OUTTAB_TABLE);
EQ_IN STEER_CTRL = OUTTAB(0, 1, 1);
EQ_IN XPREV = OUTTAB(0, 2, 1);
EQ_IN YPREV = OUTTAB(0, 3, 1);
EQ_IN IMP_STEER_SW = IF_GT_0_THEN(T, STEER_CTRL, 0);

!------------------------------------------------------------------------------------
! DYNAMIC EQUATIONS (APPLIED AFTER KINEMATICS, BEFORE FORCES AND MOMENTS)
!------------------------------------------------------------------------------------
EQ_DYN L_FORWARD = LFWD_OPTS(0, ITERS, 1);

!------------------------------------------------------------------------------------
! EQUATIONS OUT (AT THE END OF EVERY TIME STEP)
!------------------------------------------------------------------------------------
EQ_OUT XPREVIEW = XPREV;
EQ_OUT YPREVIEW = YPREV;

!------------------------------------------------------------------------------------
! EQUATIONS APPLIED AT END OF RUN
!------------------------------------------------------------------------------------
EQ_END PYRET = PYTHON(1, "steercontrol.bestval_lfwd", "OPEN", None, None);

