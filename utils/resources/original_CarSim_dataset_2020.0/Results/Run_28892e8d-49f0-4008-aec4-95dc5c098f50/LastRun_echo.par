DEFINE_OUTPUT invis = 0; - ; invis 
DEFINE_OUTPUT my_bk_1 = -1; N-m ; my_bk_1 
DEFINE_OUTPUT fd_1 = 0; N ; fd_1 
DEFINE_OUTPUT mz_kp_1 = -1.22682e-15; - ; mz_kp_1 
DEFINE_OUTPUT fs_1 = 3335.39; - ; fs_1 
DEFINE_OUTPUT fx_1 = 1.78044e-11; - ; fx_1 
DEFINE_OUTPUT fy_1 = -0; - ; fy_1 
DEFINE_OUTPUT fz_1 = 3621.82; - ; fz_1 
DEFINE_OUTPUT mx_1 = 0; - ; mx_1 
DEFINE_OUTPUT mz_1 = 0; - ; mz_1 

!------------------------------------------------------------------------------------
! EQUATIONS OUT (AT THE END OF EVERY TIME STEP)
!------------------------------------------------------------------------------------
EQ_OUT MY_BK_1 = MY_BK_L1 + MY_BK_R1;
EQ_OUT FD_1 = FD_L1 + FD_R1;
EQ_OUT MZ_KP_1 = MZ_KP_L1 + MZ_KP_R1;
EQ_OUT FS_1 = FS_L1 + FS_R1;
EQ_OUT FX_1 = FX_L1 + FX_R1;
EQ_OUT FY_1 = FY_L1 + FY_R1;
EQ_OUT FZ_1 = FZ_L1 + FZ_R1;
EQ_OUT MX_1 = MX_L1 + MX_R1;
EQ_OUT MZ_1 = MZ_L1 + MZ_R1;


