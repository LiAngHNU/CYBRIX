!------------------------------------------------------------------------------------
! NEW VARIABLES DEFINED AT RUN TIME
!------------------------------------------------------------------------------------
DEFINE_PARAMETER WP1_X = 0; m ;
DEFINE_PARAMETER WP1_Y = 0; m ;
DEFINE_PARAMETER WP1_Z = 0; m ;
DEFINE_PARAMETER WP2_X = 76.504; m ;
DEFINE_PARAMETER WP2_Y = 0; m ;
DEFINE_PARAMETER WP2_Z = 5.642; m ;
DEFINE_PARAMETER LIM = 6.5; m ;
DEFINE_PARAMETER S_LIMIT = 5; m ;
DEFINE_PARAMETER S_CURRENT = 0; - ;
DEFINE_PARAMETER RBD_MULT = 2.5; - ;
DEFINE_PARAMETER THROT_REF = 0; - ;

DEFINE_VARIABLE RB_INIT = 0; - ;
DEFINE_VARIABLE RB_PF = 0; - ;
DEFINE_VARIABLE RB_COMPL = 0; - ;
DEFINE_VARIABLE DRIVEOFF = 0; - ;
DEFINE_VARIABLE ROLL_BACK_DISTANCE = -20; - ;
DEFINE_VARIABLE PB_ON = 0; - ;
DEFINE_VARIABLE P_TARGET = 0; MPa ;
DEFINE_VARIABLE P_STATE = 0; MPa ;
DEFINE_VARIABLE PMAX = 7; MPa ;
DEFINE_VARIABLE TC_PB = 0.06; sec ;

DEFINE_OUTPUT dist_1 = 20; m ; dist_1 
DEFINE_OUTPUT dist_2 = 96.504; m ; dist_2 
DEFINE_OUTPUT outside = 0; - ; outside 
DEFINE_OUTPUT rbinit = 0; - ; rbinit 
DEFINE_OUTPUT rbpf = 0; - ; rbpf 
DEFINE_OUTPUT rbcompl = 0; - ; rbcompl 
DEFINE_OUTPUT drvoff = 0; - ; drvoff 
DEFINE_OUTPUT SLimit = 5; - ; SLimit 
DEFINE_OUTPUT SCurrent = 0; - ; SCurrent 
DEFINE_OUTPUT rb_dist = -20; - ; rb_dist 
DEFINE_OUTPUT rbdmult = -50; - ; rbdmult 
DEFINE_OUTPUT rbdcheck = 0; - ; rbdcheck 
DEFINE_OUTPUT pb_plot = 0; - ; pb_plot 
DEFINE_OUTPUT ThrotRef = 0; - ; ThrotRef 

!------------------------------------------------------------------------------------
! INITIALIZATION EQUATIONS (APPLIED JUST AFTER INITIALIZATION)
!------------------------------------------------------------------------------------
EQ_INIT ROAD_ID_OBJ(1) = CURRENT_ROAD_ID;

!------------------------------------------------------------------------------------
! EQUATIONS IN (AT THE START OF EVERY TIME STEP)
!------------------------------------------------------------------------------------
EQ_IN OUTSIDE = (DIST_1 > LIM) & (DIST_2 > LIM);
EQ_IN ROLL_BACK_DISTANCE = STATION -S_CURRENT;
EQ_IN P_TARGET = PB_ON*PMAX;
EQ_IN IMP_PBK_L2 = P_STATE;
EQ_IN IMP_PBK_R2 = IMP_PBK_L2;

!------------------------------------------------------------------------------------
! EQUATIONS OUT (AT THE END OF EVERY TIME STEP)
!------------------------------------------------------------------------------------
EQ_OUT DIST_1 = SQRT((XO -WP1_X)^2 + (YO -WP1_Y)^2);
EQ_OUT DIST_2 = SQRT((XO -WP2_X)^2 + (YO -WP2_Y)^2);
EQ_OUT RBINIT = RB_INIT;
EQ_OUT RBPF = RB_PF;
EQ_OUT RBCOMPL = RB_COMPL;
EQ_OUT DRVOFF = DRIVEOFF;
EQ_OUT SLIMIT = S_LIMIT;
EQ_OUT SCURRENT = S_CURRENT;
EQ_OUT RB_DIST = ROLL_BACK_DISTANCE;
EQ_OUT RBDMULT = RBD_MULT*ROLL_BACK_DISTANCE;
EQ_OUT RBDCHECK = IF_GT_0_THEN(RB_INIT, STATION -S_CURRENT, 0);
EQ_OUT PB_PLOT = PB_ON;
EQ_OUT THROTREF = THROT_REF;
EQ_OUT X_OBJ_1 = X_DM_1;
EQ_OUT Y_OBJ_1 = Y_DM_1;

!------------------------------------------------------------------------------------
! DIFFERENTIAL EQUATIONS FOR NEW STATE VARIABLES (AT THE END OF EVERY TIME STEP)
!------------------------------------------------------------------------------------
EQ_DIFFERENTIAL P_STATE = (P_TARGET -P_STATE)/TC_PB;

!------------------------------------------------------------------------------------
! EVENTS
!------------------------------------------------------------------------------------
! Each event is defined with a formula and an optional pathname for a parsfile to
! read if the specified formula is not zero. If no pathname is specified and the
! specified formula is not zero, then the run stops.
SET_EVENT_ID 0
DEFINE_EVENT DIST_2 < LIM; Events\Events_c7a3d3ed-cd8d-42fe-8c9c-bcb4003e6531.par ! ID 0

!------------------------------------------------------------------------------------
! IMPORTED VARIABLES, RELATIONS TO NATIVE VARIABLES, INITIAL VALUES, and UNITS
!------------------------------------------------------------------------------------
IMPORT IMP_PBK_L2 ADD 0 ; MPa ! #1. Brake actuator pressure L2
IMPORT IMP_PBK_R2 ADD 0 ; MPa ! #2. Brake actuator pressure R2


