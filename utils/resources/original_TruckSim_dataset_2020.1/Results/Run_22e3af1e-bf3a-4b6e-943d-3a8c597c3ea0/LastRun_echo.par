!------------------------------------------------------------------------------------
! VS TERRAIN
!------------------------------------------------------------------------------------
VS_TERRAIN_FILE Scene_Import\VS_Scene_Builder\Mcity_scene.vsterrain ;

!------------------------------------------------------------------------------------
! NEW VARIABLES DEFINED AT RUN TIME
!------------------------------------------------------------------------------------
DEFINE_VARIABLE V_CMD_OBJ_1 = 0; km/h ; Speed command
DEFINE_VARIABLE HEADV_O_1 = -1.5708; - ; Heading vector for ODEs
DEFINE_VARIABLE COND_START_OBJ_1 = 0; - ; Boolean to start motion
DEFINE_VARIABLE COND_STOP_OBJ_1 = 0; - ; Boolean to stop motion

!------------------------------------------------------------------------------------
! INITIALIZATION EQUATIONS (APPLIED JUST AFTER INITIALIZATION)
!------------------------------------------------------------------------------------
EQ_INIT HEADO_1 = -90/DR;
EQ_INIT S_OBJ_1 = 72.9;
EQ_INIT LATO_1 = 35;

!------------------------------------------------------------------------------------
! EQUATIONS OUT (AT THE END OF EVERY TIME STEP)
!------------------------------------------------------------------------------------
EQ_OUT V_CMD_OBJ_1 = IF(COND_START_OBJ_1 & ~COND_STOP_OBJ_1, 5.1, 0);

!------------------------------------------------------------------------------------
! DIFFERENTIAL EQUATIONS FOR NEW STATE VARIABLES (AT THE END OF EVERY TIME STEP)
!------------------------------------------------------------------------------------
EQ_DIFFERENTIAL LATO_1 = -V_CMD_OBJ_1;

!------------------------------------------------------------------------------------
! EQUATIONS APPLIED AT THE END OF EVERY FULL TIME STEP
!------------------------------------------------------------------------------------
EQ_FULL_STEP COND_START_OBJ_1 = GT(T, 0.01);
EQ_FULL_STEP COND_STOP_OBJ_1 = GT(LATO_1, 114);


END
